{
  "name": "master_mind",
  "version": "0.0.0",
  "scripts": {
    "dev": "node --require ts-node/register src/app.ts",
    "start": "node src/app.js",
    "test": "_mocha --timeout 5000 -r ts-node/register ./*/*.spec.ts",
    "eslint": "npx eslint src/*/** --fix",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "cover": " rimraf .nyc_output  && rimraf docs && nyc npm test"
  },
  "private": true,
  "dependencies": {
    "xstate": "^4.19.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.6",
    "@types/node": "10.14.6",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.2.0",
    "chai-string": "1.5.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.2",
    "mocha": "^8.4.0",
    "nock": "^12.0.3",
    "nyc": "15.0.0",
    "ts-loader": "4.2.0",
    "ts-node": "^6.2.0",
    "typescript": "3.6.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "engines": {
    "node": ">=10.15.3",
    "npm": ">=6.4.1"
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
      "src/**/*.d.ts",
      "src/app.ts"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "docs/reports/coverage",
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  }
}
